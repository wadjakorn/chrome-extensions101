{
    "manifest_version": 3,
    "name": "Focus Mode",
    "description": "Enable reading mode on Chrome's official Extensions and Chrome Web Store documentation.",
    "version": "1.0",
    "icons": {
      "16": "images/meditate-16.png",
      "32": "images/meditate-32.png",
      "48": "images/meditate-48.png",
      "128": "images/meditate-128.png"
    },
    "action": {
      "default_icon": {
        "16": "images/meditate-16.png",
        "32": "images/meditate-32.png",
        "48": "images/meditate-48.png",
        "128": "images/meditate-128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting"],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+U",
          "mac": "Command+U"
        }
      }
    }
  }